# Clawdbot Gateway Configuration
# Copy this file to .env.local and fill in your values

# WebSocket URL for Clawdbot Gateway
# Local: ws://127.0.0.1:18789
# Tailscale: ws://your-machine.tailscale-name.ts.net:18789
# Public: wss://your-domain.com:18789
NEXT_PUBLIC_GATEWAY_URL=ws://127.0.0.1:18789

# Gateway authentication token
# Get this from your Clawdbot config
NEXT_PUBLIC_GATEWAY_TOKEN=your-token-here

# Session key for chat persistence
NEXT_PUBLIC_SESSION_KEY=jarvis-ui
